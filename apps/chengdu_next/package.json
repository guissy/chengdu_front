{
  "name": "chengdu_next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3002",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "openapi1": "npx next-openapi-gen generate",
    "openapi2": "npx next-swagger-doc-cli next-swagger-doc.json",
    "generate-api": "npx openapi-typescript-codegen --input http://localhost:3001/docs/json --output lib/api",
    "api": "npx openapi-typescript http://localhost:3001/docs/json --output lib/api/schema.d.ts",
    "test": "NODE_ENV=test vitest",
    "generate-proto": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_opt=stringEnums=true --ts_proto_out=./api-proto --ts_proto_opt=esModuleInterop=true,forceLong=string,outputServices=grpc-js --proto_path=./schemas ./schemas/*.proto"
  },
  "dependencies": {
    "chengdu_ui": "workspace:*",
    "@formkit/auto-animate": "^0.8.2",
    "@hookform/resolvers": "^4.1.3",
    "@prisma/client": "^6.5.0",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@scalar/api-reference-react": "^0.6.3",
    "@scalar/nextjs-api-reference": "^0.7.3",
    "@tanstack/react-query": "^5.68.0",
    "@tanstack/react-query-devtools": "^5.68.0",
    "@tanstack/react-query-next-experimental": "^5.68.0",
    "@tanstack/react-table": "^8.21.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cypress": "^14.2.0",
    "daisyui": "^5.0.6",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "lucide-react": "^0.483.0",
    "nanoid": "^5.1.5",
    "next": "15.2.2",
    "next-swagger-doc": "^0.4.1",
    "openapi-fetch": "^0.13.5",
    "prisma": "^6.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "sharp": "^0.33.5",
    "supertest": "^7.0.0",
    "swagger-ui": "^5.20.1",
    "swagger-ui-react": "^5.20.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "ts-pattern": "^5.6.2",
    "ts-proto": "^2.6.1",
    "tw-animate-css": "^1.2.4",
    "vitest": "^3.0.9",
    "xlsx": "^0.18.5",
    "zod": "^3.24.2",
    "zod-to-json-schema": "^3.24.4",
    "zod-to-ts": "^1.2.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@openapitools/openapi-generator-cli": "^2.18.4",
    "@tailwindcss/postcss": "^4.0.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-icons": "^3.0.0",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-react": "^5.18.0",
    "eslint": "^9",
    "eslint-config-next": "15.2.2",
    "next-openapi-gen": "^0.1.2",
    "openapi-typescript": "^7.6.1",
    "openapi-typescript-codegen": "^0.29.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
